	note = {Number: 4},
	keywords = {Annelida, Earthworm, Neoblast, Stem cell, Ej-piwi, Ej-vlg1, Ej-vlg2, Embryogenesis, Enchytraeus japonensis, Germ cell, Germline, Germline stem cell, Gonadal regeneration, Growth zone, Oligochaete, Vasa-related gene},
