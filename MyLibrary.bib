@article{prentis_case_1901,
	title = {A case of incomplete duplication of parts and apparent regulation in {Nereis} {Virens} {Sars}},
	volume = {35},
	number = {415},
	journal = {The American Naturalist},
	author = {Prentis, C.W.},
	year = {1901},
	note = {Number: 415},
	pages = {563--574},
	annote = {Encuentra un ejemplar de Nereis Virens que tiene, 14 segmentos del medio del cuerpo con un juego extra de parápodos y con dos zonas ventrales. Dice que hay evidencia de una cicatriz, por lo que lo relaciona con la regeneración.Los dibujos de cortes histológicos muestran que al principio de la porción anomal hay hay dós regiones ventrales (con cadena nerviosa [ligeramente anormal], vaso ventral y musculatura longitudinal ventral) sin que sea posible saber con seguridad cual es la original (aunque una es ligeramnte más pequeña). Sin embargo, cuando progresa a lo largo de la porción anomal, una de las partes ventrales a desaparecido (la izquierda), y los parápodos adicionales están más juntos y son más pequeños. Sigue avanzando y los parápodos adicionales se van reduciendo ahsta desaparecer. Es interesante que la cadena nerviosa adicional no está conectada a la normal, y el vaso sanguineo sólo lo está anteriormente.Comenta algunos de los papers citados por Andrews 1892. Además, está de acuerdo con Andrews en que este tipo de procesos de multiplicidad relacionados con la Regenración, está relacionados con las ramificaciones de S ramosa."From our description of this single case of abnormality inNereis, and comparison of its structure with that found in bifid Polycheta, we may conclude:1. That a single metamere of a polychaete annelid may giverise to a complete or partial duplication of the main axis of the body.2. The production of these abnormalities in all probabilitytakes place in the post-embryonic worm, and is due to traumatic stimulus.3. The supernumerary, and in some cases useless, parts thus formed may be gradually suppressed by regulative changes"},
}

@article{mackie_aggregates_1986,
	title = {From aggregates to integrates: physiological aspects of modularity in colonial animals.},
	volume = {313},
	doi = {10.1098/rstb.1986.0032},
	abstract = {Physiological adaptations of colonies in the areas of resource-sharing, defensive behaviour and locomotion are reviewed. Exchange of nutrients between zooids is a fundamental attribute of true colonies, although the transport systems involved vary. Almost all colonies show some form of behavioural coordination but, again, pathways vary considerably. Defensive responses are the commonest form of coordinated activity in colonies, but some colonies show coordinated locomotion. The requirements of pelagic locomotion have led planktonic colonies to diverge markedly from the pattern seen in sessile colonies. Pelagic colonies, unlike sessile ones, are typically linear and unbranched, show an anterior-posterior axis, are determinate in form, at least at the anterior pole, and lack much of the plasticity and regenerative ability that characterize sessile colonies. -from Author},
	number = {1159},
