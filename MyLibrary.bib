@article{brockes_comparative_2008,
	title = {Comparative aspects of animal regeneration},
	volume = {24},
	issn = {1081-0706},
	url = {http://www.annualreviews.org/doi/10.1146/annurev.cellbio.24.110707.175336},
	doi = {10.1146/annurev.cellbio.24.110707.175336},
	abstract = {Most but not all phyla include examples of species that are able to regenerate large sections of the body plan. The mechanisms underlying regeneration on this scale are currently being studied in a variety of contexts in both vertebrates and invertebrates. Regeneration generally involves the formation of a wound epithelium after transection or injury, followed by the generation of regenerative progenitor cells and morphogenesis to give the regenerate. Common mechanisms may exist in relation to each of these aspects. For example, the initial proliferation of progenitor cells often depends on the nerve supply, whereas morphogenesis reflects the generation of positional disparity between adjacent cells-the principle of intercalation. These mechanisms are reviewed here across a range of contexts. We also consider the evolutionary origins of regeneration and how regeneration may relate to both agametic reproduction and to ontogeny.},
	number = {1},
	journal = {Annual Review of Cell and Developmental Biology},
	author = {Brockes, Jeremy P. and Kumar, Anoop},
	year = {2008},
	pmid = {18598212},
	note = {Number: 1},
	keywords = {planaria, salamander, stem cells, transdifferentiation, zebra fish},
	pages = {525--549},
}

