@article{isaeva_topological_2006,
	title = {Topological patterns in metazoan evolution and development},
	volume = {68},
	issn = {1153800690632},
	doi = {10.1007/s11538-006-9063-2},
	abstract = {Topological patterns in the development and evolution of metazoa, from sponges to chordates, are considered by means of previously elaborated methodology, with the genus of the surface used as a topological invariant. By this means metazoan morphogenesis may be represented as topological modification(s) of the epithelial surfaces of an animal body. The animal body surface is an interface between an organism and its environment, and topological transformations of the body surface during metazoan development and evolution results in better distribution of flows to and from the external medium, regarded as the source of nutrients and oxygen and the sink of excreta, so ensuring greater metabolic intensity. In sponges and some Cnidaria, the increase of this genus up to high values and the shaping of topologically complicated fractal-like systems are evident. In most Bilateria, a stable topological pattern with a through digestive tube is formed, and the subsequent topological complications of other systems can also appear. The present paper provides a topological interpretation of some developmental events through the use of well-known mathematical concepts and theorems; the relationship between local and global orders in metazoan development, i.e., between local morphogenetic processes and integral developmental patterns, is established. Thus, this methodology reveals a "topological imperative": A certain set of topological rules that constrains and directs biological morphogenesis.},
	number = {8},
	journal = {Bulletin of Mathematical Biology},
	author = {Isaeva, Valeria and Presnov, Eugene and Chernyshev, Alexey},
	year = {2006},
	note = {Number: 8},
	keywords = {Evolution, Genus of the surface, Morphogenesis, Ontogenesis, Topological biology, Topological surgeries},
	pages = {2053--2067},
	annote = {More ore less the same as the other two paperson topology by Isaeva. This i more or less the first on where they start to apply this formal framework to morphogenesis, ontogeny and evolution},
}

