@article{ozpolat_developmental_2016,
	title = {Developmental and molecular biology of annelid regeneration: a comparative review of recent studies},
	volume = {40},
	issn = {18790380},
	doi = {10.1016/j.gde.2016.07.010},
	abstract = {Studies of annelid regeneration have greatly increased in frequency in recent years, providing new insights into the developmental basis and evolution of regeneration. In this review, we summarize recent findings related to regeneration in annelids, focusing on molecular and developmental studies of epimorphic (blastema-based) regeneration, morphallactic (tissue-remodeling based) regeneration, and development and regeneration of putative stem cells of the posterior growth zone and germline. Regeneration is being investigated in a broad range of annelids spanning the phylum, and comparing findings among species reveals both widely conserved features that may be ancestral for the phylum as well as features that are variable across the group.},
	journal = {Current Opinion in Genetics and Development},
	author = {Özpolat, B. Duygu and Bely, Alexandra E.},
	year = {2016},
	pages = {144--153},
}

