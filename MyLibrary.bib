@article{novikova_expression_2013-1,
	title = {Expression of {Hox} genes during regeneration of nereid polychaete \textit{{Alitta}} (\textit{{Nereis}}) \textit{virens} ({Annelida}, {Lophotrochozoa})},
	volume = {4},
	issn = {2041-9139},
