	doi = {10.1371/journal.pone.0293087},
	abstract = {The polychaete Family Cirratulidae is one of the most abundant and diverse groups of Annelida, although it remains poorly known worldwide. Dodecaceria Ørsted, 1843 is one of the least described genera of Cirratulidae. The present report is the first taxonomic study of the genus Dodecaceria for the Brazilian coast. Cirratulidae were collected at Rocas Atoll, the first Brazilian marine protected area and the only atoll in the South Atlantic Ocean. We described one new species, Dodecaceria zelinhae n. sp., and a new record of D. dibranchiata Blake \& Dean, 2019, previously only known from Panama. The new species is distinguished from other Dodecaceria species by having lateral tentacles, a smooth peristomium, 3–5 pairs of branchiae, hooks from chaetiger 11 in notopodia and 9 in neuropodia. Dodecaceria dibranchiata, a Caribbean species, is here recorded for the first time in the South Atlantic Ocean.},
	language = {en},
	number = {10},
	urldate = {2023-10-31},
	journal = {PLOS ONE},
	author = {Ruta, Christine and Mundim, Davi Moreira and Freitas, Roberta and Ribeiro, Rannyele P.},
	editor = {Rubal García, Marcos},
	month = oct,
	year = {2023},
	pages = {e0293087},
	file = {Ruta et al. - 2023 - New species and record of Dodecaceria (Annelida C.pdf:/Users/rribeiro/Zotero/storage/L2XBJ8RV/Ruta et al. - 2023 - New species and record of Dodecaceria (Annelida C.pdf:application/pdf},
