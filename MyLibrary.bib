@article{gustafson_vasa_2010,
	title = {Vasa genes: {Emerging} roles in the germ line and in multipotent cells},
	volume = {32},
	issn = {1521-1878},
	doi = {10.1002/bies.201000001},
	abstract = {Sexually reproducing metazoans establish a cell lineage during development that is ultimately dedicated to gamete production. Work in a variety of animals suggests that a group of conserved molecular determinants function in this germ line maintenance and function. The most universal of these genes are vasa and vasa-like DEAD box RNA helicase genes. However, recent evidence indicates that vasa genes also function in other cell types, distinct from the germ line. Here we evaluate our current understanding of vasa function and its regulation during development, addressing vasa’s emerging role in multipotent cells. We also explore the evolutionary diversification of the amino-terminal domain of this gene and how this impacts the association of vasa with nuage-like perinuclear structures.},
	number = {7},
	journal = {BioEssays},
	author = {Gustafson, Eric A. and Wessel, Gary M.},
	year = {2010},
	note = {Number: 7},
	keywords = {Germ line, Vasa, Primordial germ cell, Multipotent cells, Zinc-knuckle},
